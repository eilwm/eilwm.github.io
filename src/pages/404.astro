---
import BaseLayout from "../layouts/BaseLayout.astro";
const base = import.meta.env.BASE_URL;
---
<BaseLayout title="Page not found — Kshitij Anand">
  <h1>404</h1>
  <p class="muted">Whoops — that page doesn't exist.</p>
  <p>
    <a href={`${base}`}>Go to Home</a> ·
    <a href={`${base}blog/`}>Blog</a> ·
    <a href={`${base}research/`}>Research</a> ·
    <a href={`${base}notes/`}>Notes</a> ·
    <a href={`${base}social/`}>Social</a>
  </p>
</BaseLayout>
